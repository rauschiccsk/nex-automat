# ANDROS Invoice Worker - Environment Variables

# Temporal Server
TEMPORAL_HOST=localhost
TEMPORAL_PORT=7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=andros-invoice-queue

# PostgreSQL Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=nex_invoices
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password_here

# MARSO API Credentials
MARSO_API_KEY=your_api_key_here
MARSO_ACCOUNT_NUM=339792
MARSO_USE_TEST=false

# ANDROS Customer
CUSTOMER_CODE=ANDROS

# Archive Path
ARCHIVE_PATH=C:/NEX/YEARACT/ARCHIV

# Invoice Pipeline (optional)
INVOICE_PIPELINE_URL=http://localhost:8000/api/v1/invoice

# Logging
LOG_LEVEL=INFO
